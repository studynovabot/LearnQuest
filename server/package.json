{
  "name": "learnquest-server",
  "version": "1.0.0",
  "description": "LearnQuest Server",
  "main": "dist/index.js",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only index.ts",
    "build": "node simple-build.cjs && node postbuild.cjs",
    "build:tsc": "tsc --skipLibCheck && node postbuild.cjs",
    "postbuild": "node postbuild.cjs",
    "predeploy": "node predeploy.cjs",
    "start": "node dist/index.js",
    "test-server": "node test-server.js",
    "seed": "ts-node-dev --transpile-only seedRunner.ts"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "firebase": "^10.8.0",
    "firebase-admin": "^11.11.1",
    "groq-sdk": "^0.3.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.0.4",
    "together-ai": "^0.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express-rate-limit": "^6.0.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.19",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
